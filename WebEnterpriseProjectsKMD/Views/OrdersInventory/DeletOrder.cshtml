@model Order

@{
    ViewData["Title"] = "DeletOrder";
}
<div class="text-center">
    <h1 class="display-4">Вы уверенны что хотите удалить заказ№@ViewBag.Code</h1>
</div>
<div>
    <form asp-action="DeletOrder" method="post">

        <input asp-for="Id" type="number" value="@ViewBag.OrderId" class="visually-hidden" /><br />
        <input asp-for="InvenoryId" type="number" value="@ViewBag.InventoryId" class="visually-hidden" /><br />

        <p align="center">
            <input type="submit" value="Удалить" style="margin:10px" />
        </p>
    </form>
</div>